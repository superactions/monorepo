{
  "name": "@superactions/deth-profiler",
  "private": "true",
  "version": "0.1.0",
  "license": "MIT",
  "main": "./src/index.ts",
  "scripts": {
    "build": "esbuild src/index.ts --bundle --outfile=dist/index.js --platform=node",
    "format": "prettier --config ../../.prettierrc --ignore-path ../../.prettierignore --check \"./**/*.ts\"",
    "format:fix": "prettier --config ../../.prettierrc --ignore-path ../../.prettierignore --write \"./**/*.ts\"",
    "lint": "eslint --ext .ts src",
    "lint:fix": "pnpm lint --fix",
    "typecheck": "tsc --noEmit --incremental false --composite false",
    "clean": "rm -rf dist && rm -f tsconfig.build.tsbuildinfo",
    "test": "true",
    "fix": "pnpm lint:fix && pnpm format:fix && pnpm test && pnpm typecheck",
    "prepublishOnly": "pnpm lint && pnpm test && pnpm build"
  },
  "dependencies": {
    "@actions/core": "^1.9.0",
    "@actions/github": "^5.0.3",
    "@deth-profiler/common": "^0.1.2",
    "@deth-profiler/profiler": "^0.1.4",
    "@superactions/artifact": "workspace:^0.1.0",
    "@superactions/comment": "workspace:^0.1.0"
  }
}
